@media (min-width: 992px) {
	.dropdown-menu {
		right: 0;
		left: auto;
		background-color: #15202b;
		border: 0;
		border-radius: 0;
		padding-top: 2rem;
		width: 15%;
		top: auto;
	}

	.dropdown-item {
		color: #fff;
		margin-bottom: 1rem;
		font-weight: 700;
		font-size: 1.3rem;
		text-align: center;
	}

	.dropdown-item:focus,
	.dropdown-item:hover {
		background-color: transparent;
		color: #33b1e3;
	}

	.dropdown, .dropleft, .dropright, .dropup {
		position: inherit;
	}

	.dropdown .dropdown-menu {
		transition: all 0.7s;
		overflow: hidden;
		opacity: 0;
		z-index: -1;
	}

	.dropdown.show .dropdown-menu {
		max-height: 300px;
		opacity: 1;
		z-index: 1;
	}
}

@media (max-width: 991px) {
	.dropdown {
		display: none;
	}
}


