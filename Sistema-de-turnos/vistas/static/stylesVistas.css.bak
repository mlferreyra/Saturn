* {
	font-family: 'Baloo Tammudu 2', cursive; 
}

html, body {
	height: 100%;
	margin: 0; 
}

.turnos-bg {
	background-image: url(../../turnos/static/imagesturnos/eldotorregistro.png);
	background-repeat: no-repeat;
	background-size: cover;
	background-position: center center;
	background-attachment: fixed;
}

.turnos-content {
	padding-top: 1rem;
}

.btn-usuarios, .btn-turno, .link-box {
	color: #fff;
	font-weight: 700;
	background-color: #15202b;
	border-color: #15202b;
}

.btn-usuarios:hover, .btn-turno:hover, .link-box:hover {
	background-color: #293D51;
	border-color: #293D51;
}

.btn-usuarios:active, .btn-turno:active, .link-box:active {
	background-color: #293D51 !important;
	border-color: #293D51 !important;
}

.turnos-content input {
	padding-top: .9rem;
	height: 2rem;
	border: 1px solid #111;
}

.turnos-content select {
	padding-top: .2rem;
	border: 1px solid #111;
}

.turnos-content input[type="date"]::-webkit-inner-spin-button {
	margin: 0;
}
.turnos-content input[type="date"]::-webkit-calendar-picker-indicator {
	margin: 0;
	padding-bottom: .6rem;
}

.btn-turno {
	padding-top: .8rem;
	width: 7rem;
	font-weight: 700;	
}

.card-title {
	font-weight: 800;
	font-size: 2.5rem;
	padding-top: 8rem;
}


.dash-box {
	background-image: url(imagenes/elwachin.png);
